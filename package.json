{
  "name": "k-create.com",
  "version": "1.0.0",
  "description": "K-Create.com | Kristoffer Hedstrom",
  "main": "n/a",
  "scripts": {
    "types": "tsc",
    "types:watch": "yarn types -- --watch",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "yarn lint && yarn types",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "transfer": "rsync --filter='- /images' --filter='- /videos' -av public/ $npm_config_deploy_path",
    "deploy": "yarn build && yarn transfer",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "author": "Kristoffer Hedstrom",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.3",
    "gatsby": "^2.25.0",
    "gatsby-link": "^2.4.3",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-sass": "^3.0.0",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-javascript-frontmatter": "^2.3.3",
    "gatsby-transformer-javascript-static-exports": "^2.4.2",
    "intersection-observer": "^0.10.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-body-classname": "^1.3.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "sass": "^1.32.4",
    "snyk": "^1.586.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "flexboxgrid": "^6.3.0",
    "prettier": "^2.0.0",
    "typescript": "^3.8.3"
  },
  "snyk": true
}
