{
  "name": "k-create.com",
  "version": "1.0.0",
  "description": "K-Create.com | Kristoffer Hedstrom",
  "main": "n/a",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "types": "tsc",
    "types:watch": "yarn types -- --watch",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "yarn lint && yarn types",
    "dev": "gatsby develop",
    "build": "npx browserslist@latest --update-db && gatsby build",
    "serve": "gatsby serve",
    "transfer": "rsync --filter='- /images' --filter='- /videos' -av public/ $npm_config_deploy_path",
    "deploy": "yarn build && yarn transfer",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Kristoffer Hedstrom",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.3.1",
    "gatsby": "^4.2.0",
    "gatsby-link": "^4.2.0",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-sass": "^5.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "gatsby-plugin-typescript": "^4.2.0",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-javascript-frontmatter": "^4.2.0",
    "gatsby-transformer-javascript-static-exports": "^4.2.0",
    "intersection-observer": "^0.12.0",
    "js-cookie": "^3.0.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-body-classname": "^1.3.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.12",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "flexboxgrid": "^6.3.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  }
}
